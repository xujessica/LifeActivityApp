<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lifeactivityapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/newlifeapplogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".FavoritesDisplayActivity"></activity>
        <activity android:name=".FavoritesActivity"/>
        <activity android:name=".DisplayActivity" />
        <activity android:name=".EntertainmentFiltersActivity" />
        <activity android:name=".EatFiltersActivity" />
        <activity android:name=".DetailedChoicesActivity" />
        <activity android:name=".ChoicesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>